{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "decorators": false,
            "dynamicImport": true
        },
        "target": "es2022",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": true
    },
    "module": {
        "type": "commonjs",
        "strict": true,
        "noInterop": false
    },
    "sourceMaps": true,
    "minify": false
}